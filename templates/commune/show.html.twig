{% extends 'base.html.twig' %}

{% block title %}Commune{% endblock %}

{% block body %}
    <h1>Commune</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ commune.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ commune.name }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ commune.lat }}</td>
            </tr>
            <tr>
                <th>Lon</th>
                <td>{{ commune.lon }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ commune.code }}</td>
            </tr>
            <tr>
                <th>Deletedat</th>
                <td>{{ commune.deletedat ? commune.deletedat|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_commune_index') }}">back to list</a>

    <a href="{{ path('app_commune_edit', {'id': commune.id}) }}">edit</a>

    {{ include('commune/_delete_form.html.twig') }}
{% endblock %}
